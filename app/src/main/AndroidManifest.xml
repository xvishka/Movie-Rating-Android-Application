<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.movierating_madcw">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MovieRatingMADCW">
        <activity android:name="com.example.movierating_madcw.SearchMovie"></activity>
        <activity android:name="com.example.movierating_madcw.EditMovie" />
        <activity android:name="com.example.movierating_madcw.FavouriteMovie" />
        <activity android:name="com.example.movierating_madcw.DisplayMovie" />
        <activity android:name="com.example.movierating_madcw.RegisterMovie" />
        <activity android:name="com.example.movierating_madcw.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>